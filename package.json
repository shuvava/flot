{
  "author": {
    "name": "Vladimir Shurgyin"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0",
    "karma": "^1.7.1",
    "karma-mocha": "^1.3.0",
    "nyc": "^11.4.1"
  },
  "scripts": {
    "build:watch": "webpack --config ./webpack.config.js  --watch",
    "build:dev": "webpack --config ./webpack.config.js",
    "build:dev:stat": "webpack --config ./webpack.config.js --env.analytics",
    "build:release": "webpack --config ./webpack.config.js --env.release",
    "test": "mocha-webpack --webpack-config webpack.config.js --webpack-env test --watch",
    "test-ci": "mocha-webpack --webpack-config webpack.config.js --webpack-env test",
    "cover": "env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test-ci"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
